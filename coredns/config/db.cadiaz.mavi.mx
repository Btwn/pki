cadiaz.mavi.mx.		IN	A	172.16.202.150
cadiaz.mavi.mx.		IN	TXT	"Algodon"
dns.cadiaz.mavi.mx.	IN	A	172.16.202.150
pc1.cadiaz.mavi.mx.	IN	A	172.16.202.150
www.cadiaz.mavi.mx.	IN	A	172.16.202.150
